#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(){
	int a, produkt, x;
	produkt = 1;
	printf("unesi viseznamenkasti broj: \n");
	scanf("%d", &a);
	while (a != 0) {
		x = a % 10;
		a = a / 10;
		produkt = produkt * x;
	}

	printf("produkt znamenki broja je %d \n", produkt);
	return 0;
}
